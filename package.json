{
  "name": "gatsby-template-sandbox",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby template sandbox",
  "author": "Chris Lanoue",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --config video=false,screenshotOnRunFailure=false",
    "e2e": "start-server-and-test dev http://localhost:8000 cypress",
    "e2e:headless": "start-server-and-test develop http://localhost:8000 cypress:headless"
  },
  "dependencies": {
    "gatsby": "5.2.0",
    "gatsby-plugin-image": "3.2.0",
    "gatsby-plugin-manifest": "5.2.0",
    "gatsby-plugin-sharp": "5.2.0",
    "gatsby-plugin-sitemap": "6.2.0",
    "gatsby-source-filesystem": "5.2.0",
    "gatsby-transformer-remark": "6.2.0",
    "gatsby-transformer-sharp": "5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@percy/cli": "1.16.0",
    "@percy/cypress": "3.1.2",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "cypress": "12.0.2",
    "start-server-and-test": "1.15.1",
    "typescript": "4.9.3"
  }
}
